{
  "layer": "top",
  "height": 35,
  "modules-left": [
    "hyprland/workspaces",
    "custom/playerctl",
    "hyprland/window"
  ],
  "modules-center": [
    "clock"
  ],
  "modules-right": [
    "tray",
    "cpu",
    "temperature",
    "memory",
    "idle_inhibitor",
    "network",
    "pulseaudio",
    "bluetooth",
    "backlight",
    "battery"
  ],
  "hyprland/window": {
    "format": "{title}",
    "max-length": 333,
    "seperate-outputs": true
  },
  "clock": {
    "format": "<span foreground='#e6b673'>ÔÄó </span><span>{:%I:%M %a %d}</span>",
    "tooltip-format": "{calendar}",
    "calendar": {
      "mode": "month",
      "mode-mon-col": 3,
      "on-scroll": 1,
      "on-click-right": "mode",
      "format": {
        "months": "<span color='#ffead3'><b>{}</b></span>",
        "days": "<span color='#ecc6d9'><b>{}</b></span>",
        "weeks": "<span color='#99ffdd'><b>{%W}</b></span>",
        "weekdays": "<span color='#ffcc66'><b>{}</b></span>",
        "today": "<span color='#ff6699'><b>{}</b></span>"
      }
    },
    "actions": {
      "on-click-middle": "mode",
      "on-click-right": "shift_up",
      "on-click": "shift_down"
    }
  },
  "cpu": {
    "format": "<span foreground='#aad94c'>Û∞Ø≥</span> {usage}%"
  },
  "memory": {
    "format": "<span foreground='#d2a6ff'>Û∞çõ</span> {}%",
    "interval": 1
  },
  "custom/gpu-util": {
    "exec": "./scripts/gpu-util",
    "format": "<span foreground='#67b0e8'>Û∞Øø</span> {}",
    "interval": 1
  },
  "custom/gpu-temp": {
    "exec": "./scripts/gpu-temp",
    "format": "<span foreground='#e57474'>Ôãâ</span> {}",
    "interval": 1
  },
  "temperature": {
    "hwmon-path": "/sys/class/hwmon/hwmon1/temp1_input",
    "critical-threshold": 80,
    "format": "<span foreground='#f29668'>Ôãâ</span> {temperatureC}¬∞C",
    "interval": 1
  },
  "backlight": {
    "device": "intel_backlight",
    "format": "<span foreground='#FFA066'>{icon}</span>  {percent}%",
    "states": [
      0,
      25,
      50,
      100
    ],
    "format-icons": [
      "ÔÜÖ",
      "Û∞Éù",
      "Û∞Éü",
      "ÔÑë"
    ],
    "tooltip-format": "{percent}%",
    "tooltip": true
  },
  "hyprland/workspaces": {
    "format": "<span> {name} {windows} </span>",
    "format-icons": {
      "default": "Ó©±",
      "urgent": "Ó©±",
      "active": "Û±ìª",
      "empty": "Ó™º"
    },
    "active-only": false,
    "sort-by-number": true,
    "on-click": "activate",
    "all-outputs": false,
    "window-rewrite-default": "ÔÅô",
    "window-rewrite": {
      "google-chrome": "Ôâ®",
      "firefox": "Ôâ©",
      "title<.*YouTube.*>": "ÔÖ™",
      "title<.*github.*>": "ÔÇõ",
      "title<.*Aniwatch.*>": "Û∞ëà",
      "kitty": "Ôíâ",
      "alacritty": "ÔÑ†",
      "code": "Û∞®û",
      "nautilus": "ÔÅª",
      "1password": "ÔÇÑ",
      "gnome-disks": "ÔÇ†",
      "gparted": "ÔÇ†",
      "polkit-gnome-authentication-agent-1": "ÔÄ£",
      "gnome-keyring-daemon": "ÔÄ£",
      "rpi-imager": "Óú¢",
      "blueman-manager": "Ôäì",
      "virt-manager": "Ó≠ª",
      "Android Studio": "Û∞Ä¥",
      "htop": "Û±ëΩ",
      "gnome-system-monitor": "Û±ëΩ"
    }
  },
  "battery": {
    "format": "<span>{icon}</span> {capacity}%",
    "format-critical": "<span foreground='#F38BA8'>{icon}</span>  {capacity}%",
    "format-icons": [
      "ÔâÑ",
      "ÔâÉ",
      "ÔâÇ",
      "ÔâÅ",
      "ÔâÄ"
    ],
    "format-time": "{H}h{M}m",
    "format-charging": "<span foreground='#a6e3a1'>{icon}</span> <span foreground='#a6e3a1'>ÔÉß</span> {capacity}% | {power} W",
    "format-full": "<span foreground='#a6e3a1'>{icon}</span> <span foreground='#a6e3a1'>ÔÉß</span> Charged",
    "interval": 1,
    "states": {
      "critical": 30
    },
    "tooltip": true,
    "on-click": "exec dunstctl history-pop"
  },
  "network": {
    "format-wifi": "Û∞§® {essid}",
    "format-ethernet": "üåê {ifname}: ‚Üï {bandwidthTotalBytes}",
    "format-linked": "Û∞§® {ifname} (No IP)",
    "format-disconnected": "Û∞§≠",
    "format-alt": "üåê {ifname}: {ipaddr}/{cidr}",
    "interval": 1,
    "tooltip": true,
    "tooltip-format-ethernet": " IP: {ipaddr} \n Network Up: {bandwidthUpBytes} \n Network Down: {bandwidthDownBytes} ",
    "tooltip-format-wifi": " Network Name: {essid} \n IP: {ipaddr} \n Network Up: {bandwidthUpBytes} \n Network Down: {bandwidthDownBytes} ",
    "on-click-right": "nm-connection-editor",
    "min-length": 22
  },
  "pulseaudio": {
    "format": "<span foreground='#f26d78'>{icon}</span> {volume}%  {format_source}",
    "format-bluetooth": "<span foreground='#95e6cb'>{icon}</span> {volume}%  {format_source}",
    "format-bluetooth-muted": "<span foreground='#95e6cb'>Û∞ñÅ</span>  {format_source}",
    "format-muted": "<span foreground='#F38BA8'>Û∞ñÅ</span>  {format_source}",
    "format-source": "<span foreground='#fab387'>ÔÑ∞</span> {volume}%",
    "format-source-muted": "<span foreground='#F38BA8'>ÔÑ±</span>",
    "format-icons": {
      "headphone": "ÔÄ•",
      "phone": "ÔÇï",
      "portable": "ÔÇï",
      "default": [
        "ÔÄ¶",
        "ÔÄß",
        "ÔÄ®"
      ]
    },
    "on-click-right": "pavucontrol",
    "on-click": "amixer set Master toggle",
    "input": true
  },
  "custom/playerctl": {
    "format": "{icon} <span>{}</span>",
    "return-type": "json",
    "max-length": 333,
    "exec": "playerctl -a metadata --format '{\"text\": \"{{artist}} ~ {{markup_escape(title)}}\", \"tooltip\": \"{{playerName}} : {{markup_escape(title)}}\", \"alt\": \"{{status}}\", \"class\": \"{{status}}\"}' -F",
    "on-click-middle": "playerctl previous",
    "on-click": "playerctl play-pause",
    "on-click-right": "playerctl next",
    "format-icons": {
      "Playing": "<span foreground='#98BB6C'>ÔÅå</span>",
      "Paused": "<span foreground='#E46876'>ÔÅã</span>"
    }
  },
  "tray": {
