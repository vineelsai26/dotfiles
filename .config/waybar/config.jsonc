    "format-icons": {
      "activated": "Û±†õ",
      "deactivated": "Û±§±"
    }
  },
  "bluetooth": {
    "on-click": "/home/vineel/.config/waybar/scripts/bluetooth-control",
    "on-click-right": "blueman-manager",
    "format": "{icon}",
    "interval": 15,
    "format-icons": {
      "on": "<span foreground='#43242B'>Ôäî</span>",
      "off": "<span foreground='#76946A'>Û∞Ç≤</span>",
      "disabled": "Û∞Ç≤",
      "connected": "Ôäì"
    },
