[core]
	editor = vim
	autocrlf = input
	eol = lf
	pager = delta
[delta]
	navigate = true
	side-by-side = true
[diff]
	colorMoved = default
[commit]
	gpgsign = true
[user]
	signingkey = ssh-ed25519 AAAAC3NzaC1lZDI1NTE5AAAAILFxoOyDEW620PrgX1atOr0N5+35im37nko3KcY+odPq
	email = mail@vineelsai.com
	name = Vineel Sai
[tag]
	gpgSign = true
[gpg]
	program = gpg
	format = ssh
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[init]
	defaultBranch = main
[credential "https://github.com"]
	helper = 
	helper = !/opt/homebrew/bin/gh auth git-credential
[credential "https://gist.github.com"]
	helper = 
	helper = !/opt/homebrew/bin/gh auth git-credential
[pull]
	rebase = false
	ff = true
[push]
	default = current

[safe]
	directory = /home/build/repo/build/neovim
[gpg "ssh"]
	program = /Applications/1Password.app/Contents/MacOS/op-ssh-sign
