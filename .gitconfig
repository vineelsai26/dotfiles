[core]
	editor = vim
	autocrlf = input
	eol = lf
[commit]
	gpgsign = true
[user]
	signingkey = ssh-ed25519 AAAAC3NzaC1lZDI1NTE5AAAAILFxoOyDEW620PrgX1atOr0N5+35im37nko3KcY+odPq
	email = mail@vineelsai.com
	name = Vineel Sai
[tag]
	gpgSign = true
[gpg]
	program = gpg
	format = ssh
[gpg "ssh"]
	program = op-ssh-sign
[push]
	autoSetupRemote = true
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[init]
	defaultBranch = main
[credential "https://github.com"]
	helper = gh auth git-credential
[credential "https://gist.github.com"]
	helper = gh auth git-credential
